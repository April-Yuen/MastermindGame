
<h1 class="text-center my-5">Score Board</h1>


  <div class="row justify-content-center">
    <div class="col-2">
      <ul class="list-unstyled">
        <li class="text-center fs-3 fw-bold">User Score</li>
        <% for(var i=0; i <users.length; i++) {%>
        <li class="text-center fs-5"><%= users[i].userScore%></li>
        <% } %>
      </ul>
    </div>
    <div class="col-2">
      <ul class="list-unstyled">
        <li class="text-center fs-3 fw-bold">Username</li>
        <% for(var i=0; i <users.length; i++) {%>
        <li class="text-center fs-5"><%= users[i].userName%></li>
        <% } %>
      </ul>
    </div>
  </div>
